{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/api/applications",
        "destination": "/applications.json"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
